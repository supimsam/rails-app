<!-- login page -->


<div class="posts">
<h1>Login</h1>
<%= form_tag '/login', method: :post do %>
	<div>
		
		<%= email_field_tag :email, nil, :placeholder => "email" %>
	</div>
	<div>
		
		<%= password_field_tag :password, nil, :placeholder => "password" %>
	</div>
	<div>
		<%= submit_tag "login" %>
	</div>
<% end %>
</div> 

